{
	"version": "2.0.0",
	"command": "C:/iRitePreprocessor/iRitePreprocessor.exe",
	"args": ["${fileDirname}/${fileBasename}", "${fileDirname}"],
	"isBackGround": false,
	"isBuildCommand": true,
	"presentation": {
		 "echo": true,
		 "reveal": "always",
		 "focus": false,
		 "panel": "shared",
		 "showReuseMessage": true
	},
	"problemMatcher": {
		 "owner": "cpp",
		 "fileLocation": ["relative", "${workspaceRoot}"],
		 "pattern": {
						 "regexp": "((([A-Za-z]):\\\\(?:[^\\/:*?\\\"<>|\\r\\n]+\\\\)*)?[^\\/\\s\\(:*?\\\"<>|\\r\\n]*)\\((\\d+)\\):\\s.*(fatal|error|warning|hint)\\s(.*):\\s(.*)",
						 "file": 1,
						 "line": 4,
						 "severity": 5,
						 "code": 6,
						 "message": 7
					}
	}
}